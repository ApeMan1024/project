package apeman1024;

import java.util.Arrays;
import java.util.Scanner;

public class Zuo1 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int year = sc.nextInt();
		int mouth=sc.nextInt();
		int day=sc.nextInt();
		
		pd(year, mouth, day);
	}
	public static void pd(int year,int mouth,int day) {
		if(year<1912||year>2050) {
			System.out.println("year越界异常");
			return;
		}else if(mouth<1||mouth>12) {
			System.out.println("mouth越界异常");
			return ;
		}else if(day<1||day>31){
			System.out.println("day越界异常");
			return ;
		}
		
		
		if(!leap(year)) {
				
			
			if(day>28) {
				if(mouth==2) {
					//非闰年，2月份，天数大于28
					System.out.println("数据错误");
				}else {
					//2月份已经额外考虑
					int n=tian(mouth);
					if(day+1>n) {
						//天数+1，大于它当前的月份的天数
						System.out.println(mouth+"."+1);
					}else {
						System.out.println(mouth+"."+(day+1));
					}
				}
			}else {
				System.out.println(mouth+"."+(day+1));
			}
		}
		else {

			if(day>29) {
				if(mouth==2) {
					//非闰年，2月份，天数大于29
					System.out.println("数据错误");
				}else {
					//2月份已经额外考虑
					int n=tian(mouth);
					if(day+1>n) {
						//天数+1，大于它当前的月份的天数
						System.out.println(mouth+"."+1);
					}else {
						System.out.println(mouth+"."+(day+1));
					}
				}
			}else {
				System.out.println(mouth+"."+(day+1));
			}
		}
	}
	/**
	 * 闰年判断
	 * @param year
	 * @return
	 */
	public static boolean leap(int year) {
		if(year%4==0&&year%100!=0) {
			return true;
		}
		return false;
	}
	public static int tian(int mouth) {
		int[] arr1= {1,3,5,7,8,10,12};
		if(Arrays.binarySearch(arr1,mouth)>=0) {
			return 31;
		}else {
			return 30;
		}
	}
}
