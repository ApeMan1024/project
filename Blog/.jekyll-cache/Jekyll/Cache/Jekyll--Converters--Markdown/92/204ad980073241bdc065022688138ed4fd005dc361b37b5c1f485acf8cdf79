<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>public static void insertSort(int arr[]) {
		for(int i=1;i&lt;arr.length;i++) {
			if(arr[i]&lt;arr[i-1]) {
				int k=arr[i];
				int j=0;
				for(j=i-1;j&gt;=0&amp;&amp;k&lt;arr[j];j--) {
					arr[j+1]=arr[j];
				}
				arr[j+1]=k;
			}
		}
    }
