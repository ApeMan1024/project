<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>	public static void selectSort(int arr[]) {
		for(int i=0;i&lt;arr.length;i++) {
			int min=i;
			int j=0;
			for(j=i+1;j&lt;arr.length;j++) {
				if(arr[min]&gt;arr[j]){
					min=j;
				}
			}
			if(min!=i) {
				int temp=arr[i];
		
				arr[i]=arr[min];
				
				arr[min]=temp;
			}
		}
	}
</code></pre></div></div>

