<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>	public static void shellSort(int []arr) {
		for(int i=arr.length/2;i&gt;0;i/=2) {
			
			for(int j=i;j&lt;arr.length;j++) {
				
				for(int k=j-i;k&gt;=0;k-=i) {
					
					if(arr[k]&gt;arr[k+i]) {
						int temp=arr[k];
						arr[k]=arr[k+i];
						arr[k+i]=temp;
					}
				}
			}
		}
	}
</code></pre></div></div>

